{
    "name": "nguyenngocchinh/cakephp4-driver-bigquery",
    "description": "BigQuery direver for CakePHP 4.4",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "cakephp-plugin",
        "bigquery"
    ],
    "require": {
        "php": ">=7.4",
        "google/cloud-bigquery": "^1.30",
        "cakephp/cakephp": "^4.4.*",
        "josegonzalez/dotenv": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "cakephp/cakephp-codesniffer": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chinh",
            "email": "chinh@duck.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chinh\\BigQuery\\": "src/",
            "Chinh\\BigQuery\\Test\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@test",
            "@cs-check"
        ],
        "cs-check": "phpcs -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "cs-fix": "phpcbf --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "test": "phpunit --stderr"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
